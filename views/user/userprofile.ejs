<script>
    document.getElementById("name").addEventListener('input',function(){
        var name=document.getElementById("name").value.trim()
        if (name==""){
            document.getElementById("name").setCustomValidity("Name cannot be null")
        }
        // else if(name.lenght<5){
        //     document.getElementById("name").setCustomValidity("Name must be 5 letters ")

        // }
        else{
            document.getElementById("name").setCustomValidity("")
        }

    })
    
    document.getElementById("email").addEventListener("input",function(){
     
      var email=document.getElementById("email").value.trim()
      console.log(email);
      if(email==""){
        document.getElementById("email").setCustomValidity("Email cannot be null")
      }else{
        document.getElementById("email").setCustomValidity("")
      }
    })
    document.getElementById("phone").addEventListener("input",function(){
      var phone=document.getElementById("phone").value.trim()
      
      if(phone.length<10){
        document.getElementById("phone").setCustomValidity("phone number lesstha 10")
     

      }else{
        document.getElementById("phone").setCustomValidity("")
  
      }
    })
   
    document.getElementById("password").addEventListener("input",function(){
      var password=document.getElementById("password").value.trim()
      
      if(password.length<5){
        document.getElementById("password").setCustomValidity("Password is less than 5")
      }else{
        document.getElementById("password").setCustomValidity("")
  
      }
    })
    document.getElementById("cpassword").addEventListener("input",function(){
      var cpassword=document.getElementById("cpassword").value.trim()
      var password=document.getElementById("password").value.trim()
      
      if(cpassword!=password){
        document.getElementById("cpassword").setCustomValidity("Password doden't match")
      }else{
        document.getElementById("cpassword").setCustomValidity("")
  
      }
    })

  
  </script>
